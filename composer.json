{
    "name": "co0lsky/image-optimizer",
    "description": "Easily optimize images using PHP 5.6",
    "keywords": [
        "spatie",
        "image-optimizer"
    ],
    "homepage": "https://github.com/co0lsky/image-optimizer",
    "license": "MIT",
    "authors": [
        {
            "name": "Sky Chin",
            "email": "sky4just@gmail.com",
            "homepage": "http://www.skychin.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6",
        "ext-fileinfo": "*",
        "psr/log": "^1.0",
        "symfony/process": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "symfony/var-dumper": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "SkyChin\\ImageOptimizer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SkyChin\\ImageOptimizer\\Test\\": "tests"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
